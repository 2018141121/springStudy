<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--    数据库的信息配置完成后就开始第二步-->
    <import resource="spring-dao.xml"/>

    <!--    实现类注入spring里面  这个一步很重要-->
    <bean id="userMapperImpl" class="com.q.Mapper.UserMapperImpl">
        <!--        相当于这个bean的实现依赖与我们定义的模板sqlSessionTemplate2-->
        <property name="sqlSessionTemplate" ref="sqlSessionTemplate2"/>


    </bean>
    <bean id="userMapperImpl02" class="com.q.Mapper.UserMapperImpl02">
        <property name="sqlSessionTemplate" ref="sqlSessionTemplate2"/>
    </bean>
    <bean class="com.q.pojo.User" id="user"/>
</beans>
